<header>
    <% include ../partials/header %>
</header>

<main>
  <div class= "container-fluid">
    <div class="row">
        <% if( (testing != null) && (start != null)){ %>
        <% for(var i=0; i < data.length; i++) { %>
        <div class= "col-sm-3">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title"> <%= data[i].eventName %></h5>
              <h6 class="card-subtitle mb-2 text-muted"> <%= data[i].timeStart %> to <%= data[i].timeEnd %> </h6>
              <p class="card-text"> <%=data[i].eventAddress%> <br> <%=data[i].eventCity%> <%=data[i].eventZip%> </p>
              <a class="btn btn-primary btn-sm" href="/event/<%= data[i].id %>" value="accept"> View </a>
            </div>
          </div>
      </div>
        <% } %>
        <% } %>

        <% if ((data == null) && (start != null)) { %>
            <div class="alert alert-warning alert-dismissible fade show col-sm-12" role="alert">
              <strong> No results found with those keywords. Please try again. </strong>
            </div>
            <%}%>
    </div>


            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                <ul class="carousel-indicators">
                  <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                  <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                  <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                </ul>
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img class="d-block w-100" src="img/cs1.jpg" alt="First slide">
                        <div class="carousel-caption d-none d-md-block">
                          <h5>Hackathon 1</h5>
                          <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit</p>
                        </div>
                  </div>
                  <div class="carousel-item">
                    <img class="d-block w-100" src="img/cs2.jpg" alt="Second slide">
